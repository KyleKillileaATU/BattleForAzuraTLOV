            />
        <Image 
            x:Name ="BackgroundLevel07"
            Source="backgroundlevel01.png" 
            WidthRequest="965"
            Aspect="AspectFill"
    
            SemanticProperties.Description="BG7" 
            />
        <Image 
            x:Name ="BackgroundLevel08"
            Source="backgroundlevel01.png" 
            WidthRequest="965"
            Aspect="AspectFill"
    
            SemanticProperties.Description="BG8" 
            />
        <BoxView x:Name ="PlayerCameraBox"
         Color ="DarkBlue"
         WidthRequest ="300"
         HeightRequest ="220"/>
        <BoxView x:Name ="PlayerHitBox"
         Color ="DarkGreen"
         WidthRequest ="90"
         HeightRequest ="90"/>
        <BoxView x:Name ="PlayerHitBoxtopleft"
         Color ="DarkOrange"
         WidthRequest ="5"
         HeightRequest ="5"/>
        <BoxView x:Name ="PlayerHitBoxtopright"
         Color ="DarkOrange"
         WidthRequest ="5"
         HeightRequest ="5"/>
        <BoxView x:Name ="PlayerHitBoxbotleft"
         Color ="DarkOrange"
         WidthRequest ="5"
         HeightRequest ="5"/>
        <BoxView x:Name ="PlayerHitBoxbotright"
         Color ="DarkOrange"
         WidthRequest ="5"
         HeightRequest ="5"/>
        <!-- projectiles  -->
        <Ellipse x:Name ="Projectile01"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <Ellipse x:Name ="Projectile02"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <Ellipse x:Name ="Projectile03"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <Ellipse x:Name ="Projectile04"
         WidthRequest="20"
