         />
        <Ellipse x:Name ="Projectile08"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <Ellipse x:Name ="Projectile09"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <Ellipse x:Name ="Projectile10"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <Ellipse x:Name ="Projectile11"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <Ellipse x:Name ="Projectile12"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile13"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile14"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile15"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile16"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile17"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile18"
         WidthRequest="20"
