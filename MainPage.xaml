         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile21"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
         <Ellipse x:Name ="Projectile22"
         WidthRequest="20"
         HeightRequest="30"
         Fill="DarkGray"
         />
        <!-- player image -->
        <Image 
            x:Name ="PlayerIMG"
            Source="walter3.png" 
            HeightRequest="100"
            WidthRequest="100"
            Aspect="AspectFill"
            
            SemanticProperties.Description="player" 
            />
        <!-- enemy's  -->
        <Image 
             x:Name ="e001"
             Source="ahomeless.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <Image 
             x:Name ="e002"
             Source="injuredsoldier.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <Image 
             x:Name ="e003"
             Source="sadman.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <Image 
             x:Name ="e004"
             Source="medipeasant.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <Image 
             x:Name ="e005"
             Source="angryfarmer.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <Image 
             x:Name ="e006"
             Source="british.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <Image 
             x:Name ="e007"
             Source="childadventuerer.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <Image 
             x:Name ="e008"
             Source="commonsoldier.png" 
             HeightRequest="45"
             Aspect="AspectFit"
             SemanticProperties.Description="enemy" 
             />
        <!-- stats ui  -->
        <HorizontalStackLayout HorizontalOptions="Start" VerticalOptions="Start" Padding="10,20,10,20" Spacing="5">
            <BoxView x:Name ="PlayerMagicbase"
              Color="#393939"
              WidthRequest ="30"
              HeightRequest ="150"/>
            <BoxView x:Name ="PlayerHPbase"
              Color="#393939"
              WidthRequest ="30"
              HeightRequest ="150"/>
            <BoxView x:Name ="PlayerStaminabase"
              Color="#393939"
              WidthRequest ="30"
              HeightRequest ="150"/>
            <BoxView x:Name ="PlayerMagicbar"
              Color="#af0039"
              WidthRequest ="25"
              HeightRequest ="140"/>
            <BoxView x:Name ="PlayerHPbar"
              Color="#39cf3a"
              WidthRequest ="25"
              HeightRequest ="140"/>
            <BoxView x:Name ="PlayerStaminabar"
              Color="#ffff39"
              WidthRequest ="25"
              HeightRequest ="140"/>
        </HorizontalStackLayout>
        <!-- menu screens go here  -->
        <Image 
         x:Name ="TitleScreen01"
         Source="dishonouredtitlescreen.png" 
         HeightRequest="550"
         Aspect="AspectFit"
         SemanticProperties.Description="title screen" 
         />
        <Image 
         x:Name ="BattleForAzuraTitle"
         Source="battleforazuratitleimg.png" 
         HeightRequest="150"
         Aspect="AspectFit"
         SemanticProperties.Description="big title" 
         />
        <Image 
         x:Name ="NewGameScreen01"
         Source="dishonourednewgamescreen.png" 
         HeightRequest="550"
         Aspect="AspectFit"
         SemanticProperties.Description="new game screen" 
         />
        <Image 
         x:Name ="ContinueScreen01"
         Source="dishonouredcontinuescreen.png" 
         HeightRequest="550"
         Aspect="AspectFit"
         SemanticProperties.Description="continue screen" 
         />
        <Image 
         x:Name ="MissionScreen01"
         Source="dishonouredmissionscreen.png" 
         HeightRequest="550"
         Aspect="AspectFit"
         SemanticProperties.Description="missions screen" 
         />
        <Image 
         x:Name ="LevelStatsScreen01"
         Source="dishonouredlevelstatsscreen.png" 
         HeightRequest="550"
         Aspect="AspectFit"
         SemanticProperties.Description="level stats screen" 
         />
        <Image 
         x:Name ="GrayFilterScreen01"
         Source="greyfilter.png" 
         HeightRequest="550"
         Aspect="AspectFit"
         SemanticProperties.Description="filter" 
         />
        <!-- screen buttons go here  -->
        <Grid>
            <Label 
                x:Name ="testnumber"
                Text="Test number: 0   "
            />
            <!-- game  -->
            <VerticalStackLayout HorizontalOptions="Start" VerticalOptions="End" Padding="10,20" Spacing="5">
                <Button x:Name="backmovebutton"  Text="Back" Clicked="BackMoveBTN_Clicked" WidthRequest="100" TextColor="OrangeRed" BackgroundColor="{ DynamicResource ColourOfBackMoveBTNClicked }"/>
                <Button x:Name="leftmovebutton"  Text="Left" Clicked="LeftMoveBTN_Clicked" WidthRequest="100" TextColor="OrangeRed" BackgroundColor="{ DynamicResource ColourOfLeftMoveBTNClicked }"/>
                <Button x:Name="rightmovebutton"  Text="Right" Clicked="RightMoveBTN_Clicked" WidthRequest="100" TextColor="OrangeRed" BackgroundColor="{ DynamicResource ColourOfRightMoveBTNClicked }"/>
                <Button x:Name="forwardmovebutton"  Text="Forwards" Clicked="MoveBTN_Clicked" WidthRequest="100" TextColor="OrangeRed" BackgroundColor="{ DynamicResource ColourOfForwardMoveBTNClicked }"/>
            </VerticalStackLayout>
            <VerticalStackLayout HorizontalOptions="End" VerticalOptions="End" Padding="10,20,10,20" Spacing="5">
                <Button x:Name="attackbutton"  Text="Shoot" Clicked="AttackBTN_Clicked" WidthRequest="80" TextColor="blue" BackgroundColor="{ DynamicResource ColourOfAttackBTNClicked }"/>
            </VerticalStackLayout>
            <VerticalStackLayout HorizontalOptions="End" VerticalOptions="Start" Padding="10,20,10,20" Spacing="5">
                <Button x:Name="gamemenubutton"  Text="Menu" Clicked="GameMenuBTN_Clicked" WidthRequest="80" TextColor="blue" BackgroundColor="{ DynamicResource ColourOfGameMenuBTNClicked }"/>
                <Button x:Name="weaponswitchbutton"  Text="Weapon" Clicked="WeaponBTN_Clicked" WidthRequest="90" TextColor="blue" BackgroundColor="{ DynamicResource ColourOfWeaponSwitchBTNClicked }"/>
                <Button x:Name="backgroundammotext"  Text="placeholder" WidthRequest="100" TextColor="Yellow" BackgroundColor="Yellow"/>
                <Label 
                    x:Name ="ammoqtext"
                    BackgroundColor="Yellow"
                    Text="Current Ammo: 0   "
                />
                
            </VerticalStackLayout>
            
                <Button x:Name="backgroundweaponmenu01"  Text="placeholder" WidthRequest="80" HeightRequest="80" TextColor="#393939" BackgroundColor="#393939"/>
                <Button x:Name="backgroundweaponmenu02"  Text="placeholder" WidthRequest="300" HeightRequest="80" TextColor="LightGray" BackgroundColor="LightGray"/>
                <Button x:Name="backgroundweaponmenu03"  Text="placeholder" WidthRequest="140" HeightRequest="80" TextColor="DarkGray" BackgroundColor="DarkGray"/>
                <Button x:Name="backgroundweaponmenu04"  Text="placeholder" WidthRequest="140" HeightRequest="80" TextColor="DarkGray" BackgroundColor="DarkGray"/>
                <Button x:Name="backgroundweaponmenu05"  Text="placeholder" WidthRequest="140" HeightRequest="80" TextColor="DarkGray" BackgroundColor="DarkGray"/>
                <Button x:Name="backgroundweaponmenu06"  Text="placeholder" WidthRequest="140" HeightRequest="80" TextColor="DarkGray" BackgroundColor="DarkGray"/>
                <Button x:Name="backgroundweaponmenu07"  Text="placeholder" WidthRequest="140" HeightRequest="80" TextColor="DarkGray" BackgroundColor="DarkGray"/>
                <Button x:Name="backgroundweaponmenu08"  Text="placeholder" WidthRequest="140" HeightRequest="80" TextColor="DarkGray" BackgroundColor="DarkGray"/>

                <Button x:Name="weaponmenu01"  Text="gun 1" Clicked="Weapon1BTN_Clicked" WidthRequest="70" HeightRequest="80" TextColor="LightGray" BackgroundColor="{ DynamicResource ColourOfWeapon1BTNClicked }"/>
                <Button x:Name="weaponmenu02"  Text="gun 2" Clicked="Weapon2BTN_Clicked"  WidthRequest="70" HeightRequest="80" TextColor="LightGray" BackgroundColor="{ DynamicResource ColourOfWeapon2BTNClicked }"/>
                <Button x:Name="weaponmenu03"  Text="gun 3" Clicked="Weapon3BTN_Clicked"  WidthRequest="70" HeightRequest="80" TextColor="LightGray" BackgroundColor="{ DynamicResource ColourOfWeapon3BTNClicked }"/>
                <Button x:Name="weaponmenu04"  Text="gun 4" Clicked="Weapon4BTN_Clicked"  WidthRequest="70" HeightRequest="80" TextColor="LightGray" BackgroundColor="{ DynamicResource ColourOfWeapon4BTNClicked }"/>
                <Button x:Name="weaponmenu05"  Text="gun 5" Clicked="Weapon5BTN_Clicked"  WidthRequest="70" HeightRequest="80" TextColor="LightGray" BackgroundColor="{ DynamicResource ColourOfWeapon5BTNClicked }"/>
                <Button x:Name="weaponmenu06"  Text="gun 6" Clicked="Weapon6BTN_Clicked"  WidthRequest="70" HeightRequest="80" TextColor="LightGray" BackgroundColor="{ DynamicResource ColourOfWeapon6BTNClicked }"/>

            <!-- main  -->
            <Button x:Name="NewGamebutton"  Text="New Game" Clicked="NGameBTN_Clicked" WidthRequest="120" HeightRequest ="30" TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfNewGameBTNClicked }"/>
            <Button x:Name="Continuebutton"  Text="Continue" Clicked="ConBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfContinueBTNClicked }"/>
            <Button x:Name="Trainingbutton"  Text="Testing Area" Clicked="TrainBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfTrainingBTNClicked }"/>
            <Button x:Name="Missionbutton"  Text="Missions" Clicked="MissionBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfMissionBTNClicked }"/>
            <Button x:Name="SuperShopbutton"  Text="Super Store" Clicked="SShopBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfSuperShopBTNClicked }"/>
            <Button x:Name="Brutalbutton"  Text="Brutal" Clicked="BrutaBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfBrutalBTNClicked }"/>
            <Button x:Name="Challengebutton"  Text="Challenges" Clicked="ChallBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfChallengeBTNClicked }"/>
            <Button x:Name="Musicbutton"  Text="BGM Custom" Clicked="MusicBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfMusicBTNClicked }"/>
            <Button x:Name="Settingsbutton"  Text="Options" Clicked="SettingsBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfSettingsBTNClicked }"/>
            
            <!-- new game  -->
            <Button x:Name="easydiffbutton"  Text="Easy" Clicked="EasyDBTN_Clicked" WidthRequest="220" HeightRequest ="15" TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfEasyBTNClicked }"/>
            <Button x:Name="normaldiffbutton"  Text="Normal" Clicked="NormDBTN_Clicked" WidthRequest="220" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfNormalBTNClicked }"/>
            <Button x:Name="harddiffbutton"  Text="Hard" Clicked="HardDBTN_Clicked" WidthRequest="220" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfHardBTNClicked }"/>
            <Button x:Name="veryharddiffbutton"  Text="Very Hard" Clicked="VHardDBTN_Clicked" WidthRequest="220" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfVeryHardBTNClicked }"/>
            <Button x:Name="accept01button"  Text="Accept" Clicked="Accept01BTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            <Button x:Name="leavebutton"  Text="Exit" Clicked="EscapeBTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            
            <!-- Continue game  -->
            <Button x:Name="saveslot1button"  Text="Save 1" Clicked="Save1BTN_Clicked" WidthRequest="150" HeightRequest ="15" TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfSave1BTNClicked }"/>
            <Button x:Name="saveslot2button"  Text="Save 2" Clicked="Save2BTN_Clicked" WidthRequest="150" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfSave2BTNClicked }"/>
            <Button x:Name="saveslot3button"  Text="Save 3" Clicked="Save3BTN_Clicked" WidthRequest="150" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfSave3BTNClicked }"/>
            <Button x:Name="deletesavebutton"  Text="Delete" Clicked="DelSaveBTN_Clicked" WidthRequest="80" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            <Button x:Name="accept02button"  Text="Accept" Clicked="Accept02BTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            <Button x:Name="leave02button"  Text="Exit" Clicked="EscapeBTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            <!-- Testing Area  -->
            <Button x:Name="accept03button"  Text="Accept" Clicked="Accept03BTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            <Button x:Name="leave03button"  Text="Exit" Clicked="EscapeBTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>

            <!-- Missions  -->
            <Button x:Name="previousmissionbutton"  Text=" ← " Clicked="PrevMissBTN_Clicked" WidthRequest="50" HeightRequest ="15" TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfPrevMissBTNClicked }"/>
            <Button x:Name="nextmissionbutton"  Text=" → " Clicked="NextMissBTN_Clicked" WidthRequest="50" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfNextMissBTNClicked }"/>
            <Button x:Name="missionstatsbutton"  Text="Stats" Clicked="MissStatsBTN_Clicked" WidthRequest="80" HeightRequest ="15"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            <Button x:Name="accept04button"  Text="Accept" Clicked="Accept04BTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            <Button x:Name="leave04button"  Text="Exit" Clicked="EscapeBTN_Clicked" WidthRequest="80" HeightRequest ="10"  TextColor="#a9a9a9" BackgroundColor="DarkSlateGrey"/>
            
            <!-- Title Screen  -->
            <Image 
             x:Name ="BigTitleScreen01"
             Source="greyfilter.png" 
             HeightRequest="550"
             Aspect="AspectFit"
             SemanticProperties.Description="Title Screen Here" 
             />
            <Image 
             x:Name ="TitleScreen02"
             Source="battleforazuratitleimg.png" 
             HeightRequest="150"
             Aspect="AspectFit"
             SemanticProperties.Description="title screen" 
             />
            <Button x:Name="EnterGamebutton"  Text="Enter" Clicked="StartBTN_Clicked" WidthRequest="280" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="{ DynamicResource ColourOfStartGameBTNClicked }"/>
            
        </Grid>
        <!-- archived
        <Button x:Name="NewTutorialbutton"  Text="Tutorial" Clicked="NTutorBTN_Clicked" WidthRequest="120" HeightRequest ="30"  TextColor="#a9a9a9" BackgroundColor="#393939"/>
-->
    </Grid>
    
                
</ContentPage>
